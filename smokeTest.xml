	</test>
	<test verbose="2" name="Yes Bank test">

		<groups>
			<run>
				<include name="smokeTest" />
			</run>
		</groups>


		<classes>
			<class name="YesBank.WDYesBank" />
			<class name="YesBank.LoanYesBank" />
			<class name="YesBank.AllocationYesBank" />
		</classes>
	</test>
